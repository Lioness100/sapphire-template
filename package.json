    "dotenv": "^10.0.0",
    "prompts": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.1",
    "tslog": "^3.2.0",
    "wordwrapjs": "^5.0.2",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@lioness100/eslint-config": "^1.0.10",
    "@lioness100/prettier-config": "^1.0.0",
    "@lioness100/ts-config": "1.0.3",
    "@types/node": "^16.6.1",
    "esbuild": "^0.12.20",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.3.4"
  },
