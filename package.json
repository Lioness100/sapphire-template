  "license": "MIT",
  "type": "module",
  "private": true,
  "imports": {
    "#root/*": "./dist/*.js",
    "#config/*": "./dist/config/*.js",
    "#lib/*": "./dist/lib/*.js",
    "#types/*": "./dist/lib/types/*.js",
    "#structures/*": "./dist/lib/structures/*.js",
    "#factories/*": "./dist/lib/factories/*.js",
