    "#repositories/*": "./dist/lib/database/repositories/*.js"
  },
  "scripts": {
    "start": "node .",
    "lint": "eslint src --ext ts --fix",
    "format": "prettier --write --loglevel=warn \"src/**/*.ts\"",
    "update": "yarn upgrade-interactive --latest",
    "build": "node scripts/build.mjs",
    "watch": "node scripts/watch.mjs",
    "clean": "node scripts/clean.mjs",
    "setup": "run-s clean build start",
    "test": "run-s lint typecheck",
    "typecheck": "tsc -p src --noEmit",
    "cmd-gen": "node scripts/generators/command.mjs",
    "piece-gen": "node scripts/generators/piece.mjs"
  },
  "dependencies": {
    "@mikro-orm/core": "^4.5.7",
